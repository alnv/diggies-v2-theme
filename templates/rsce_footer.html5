<script>
    (function () {
        const allBtns = document.querySelectorAll(".information_wrapper-links_box");

        allBtns.forEach((el) => {
            el.querySelector('.mobile.icon-plus').addEventListener("click", () => {
                if (el.querySelector(".links_box-list")) {
                    el.querySelector(".links_box-list").classList.toggle("is-hidden-mobile");
                }
                if (el.querySelector(".contact_box-list_wrapper")) {
                    el.querySelector(".contact_box-list_wrapper").classList.toggle("is-hidden-mobile");
                }
                el.querySelector(".icon-plus").classList.toggle("is-hidden");
                el.querySelector(".icon-minus").classList.toggle("is-hidden");
            })
            el.querySelector('.mobile.icon-minus').addEventListener("click", () => {
                if (el.querySelector(".links_box-list")) {
                    el.querySelector(".links_box-list").classList.toggle("is-hidden-mobile");
                }
                if (el.querySelector(".contact_box-list_wrapper")) {
                    el.querySelector(".contact_box-list_wrapper").classList.toggle("is-hidden-mobile");
                }
                el.querySelector(".icon-plus").classList.toggle("is-hidden");
                el.querySelector(".icon-minus").classList.toggle("is-hidden");
            })
        })
    })();
</script>